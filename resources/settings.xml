                    <default></default>
                    <constraints>
                        <writable>true</writable>
                        <allowempty>true</allowempty>
                    </constraints>
                    <control type="button" format="path">
                        <heading>30105</heading>
                    </control>
                </setting>
                <setting id="log_level" type="integer" label="30129" help="">
