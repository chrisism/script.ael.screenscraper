                <setting id="scraper_screenscraper_ssid" type="string" label="30101" help="">
                    <level>0</level>
                    <default></default>
                    <constraints>
                        <allowempty>true</allowempty>
                    </constraints>
                    <control type="edit" format="string">
                        <heading>30101</heading>
                    </control>
                </setting>
                <setting id="scraper_screenscraper_sspass" type="string" label="30102" help="">
                    <level>0</level>
                    <default></default>
                    <constraints>
                        <allowempty>true</allowempty>
                    </constraints>
                    <control type="edit" format="string">
                        <heading>30101</heading>
                    </control>
                </setting>
                <setting id="scraper_screenscraper_region" type="integer" label="30103" help="">
                    <level>1</level>
                    <default>1</default>
                    <constraints>
                        <options>
                            <option label="30911">0</option>
                            <option label="30912">1</option>
                            <option label="30913">2</option>
                            <option label="30914">3</option>
                            <option label="30915">4</option>
                        </options>
                    </constraints>
                    <control type="spinner" format="string"/>
                </setting>
                <setting id="scraper_cache_dir" type="path" label="30105" help="">
